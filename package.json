  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rolyatmax/rota/issues"
  },
  "homepage": "https://github.com/rolyatmax/rota",
  "devDependencies": {
    "browserify": "13.0.0",
    "eslint": "2.8.0",
    "hbsify": "1.0.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "underscore": "1.7.0"
  },
  "browserify": {
    "transform": [
      "hbsify"
    ]
  }
