{
  "name": "rota",
  "version": "0.0.2",
  "description": "Routing with reinforcement learning",
  "main": "js/main.js",
  "scripts": {
    "build": "browserify . -o build/build.js",
    "watch": "watchify . --full-paths -o build/build.js -dv",
